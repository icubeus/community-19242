<?xml version="1.0"?>
<config>
    <modules>
        <Icube_Sociallink>
            <version>0.1.0</version>
        </Icube_Sociallink>
    </modules>
    <global>
	    <helpers>
            <icube_sociallink>
                <class>Icube_Sociallink_Helper</class>
            </icube_sociallink>
        </helpers>
        <models>
	        <icube_sociallink>
                <class>Icube_Sociallink_Model</class>
                <resourceModel>icube_sociallink_resource</resourceModel>
            </icube_sociallink>
            <icube_sociallink_resource>
                <class>Icube_Sociallink_Model_Resource</class>
                <entities>
                    <sociallink>
                        <table>icube_sociallink</table>
                    </sociallink>
                </entities>
            </icube_sociallink_resource>
        </models>
        <resources>
            <icube_sociallink_setup>
                <setup>
                    <module>Icube_Sociallink</module>
                </setup>
            </icube_sociallink_setup>
        </resources>
    </global>
    <adminhtml>
	    <events>
            <adminhtml_catalog_product_grid_prepare_massaction>
                <observers>
                    <add_massaction_product_grid>
                        <class>icube_sociallink/observer</class>
                        <type>singleton</type>
                        <method>addMassAction</method>
                    </add_massaction_product_grid>
                </observers>
            </adminhtml_catalog_product_grid_prepare_massaction>
        </events>
    </adminhtml>
</config>